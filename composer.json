{
    "name": "nmeri/suphle-starter",
    "type": "project",
    "description": "Setup Suphle folder structure and provide workers or entry scripts for handling incoming requests",
    "autoload": {
        "psr-4": {
            "AllModules\\": "AllModules/",
            "AppModels\\": "AppModels/",
            "ModuleInteractions\\": "ModuleInteractions/"
        }
    },

    "repositories": [
    	{
    		"type": "path",
	        "url": "../suphle"
	    }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "mcaskill/composer-exclude-files": true
        },
        "sort-packages": true
    },
    "require": {
        "nmeri/suphle": "*"
    },
    "require-dev": {
        "wyrihaximus/json-throwable": "^4.1",
        "fakerphp/faker": "^1.19",
        "phpunit/phpunit": "^8.5",
        "vimeo/psalm": "*"
    }
}
